go run . -video_size 2560x1440 -framerate 25 -probesize 42M -f x11grab -i :0.0+ -f pulse -ac 2 -i default -pix_fmt yuv420p -c:v libx264 -bsf:v h264_mp4toannexb -b:v 2M -max_delay 0 -bf 0 -f h264 - <SDP.txt
go run . -video_size 2560x1440 -framerate 25 -probesize 42M -f x11grab -thread_queue_size 4096 -i :0.0+ -f pulse -thread_queue_size 4096 -ac 2 -i default -pix_fmt yuv420p -b:v 2M -max_delay 0 -bf 0 -f ivf - <SDP.txt